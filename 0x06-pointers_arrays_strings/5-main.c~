#include "holberton.h"
#include <stdio.h>

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
int main(void)
{
	char *s = "hello, wo world lord";
	char *f = "hello";
	char *t;

	t = _strstr(s, f);
	printf("%s\n", t);

	char *a = "hello, qu world lord";
	char *b = "wo";
	char *c;

	c = _strstr(a, b);
	printf("%s\n", c);

	return (0);
}
